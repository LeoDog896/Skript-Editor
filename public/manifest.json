{
  "short_name": "skEditor",
  "name": "Skript Editor",
  "description": "An app to edit Skript!",
  "version": "0.1",
  "manifest_version": 2,
  "icons": [
    {
      "src":"images/logo.svg",
      "sizes": "512x512",
      "type": "image/svg"
    },
    {
      "src":"images/logo.svg",
      "sizes": "192x192",
      "type": "image/svg"
    }
  ],
  "start_url": "/app",
  "offline_enabled": true,
  "permissions": [
    "unlimitedStorage"
  ],
  "background_color": "#000",
  "theme_color": "#536878",
  "display": "standalone"
}
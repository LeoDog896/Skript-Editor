{
  "shortname": "skEdit",
  "name": "Skript Editor",
  "description": "An app to edit Skript!",
  "version": "0.1",
  "manifest_version": 2,
  "icons": [
    {
      "src":"logo.svg",
      "sizes": "32x32",
      "type": "image/svg"
    }
  ],
  "app": {
    "urls": [
      "https://skript-editor.glitch.me"
    ],
    "launch": {
      "web_url": "https://skript-editor.glitch.me"
    }
  },
  "offline_enabled": true,
  "permissions": [
    "unlimitedStorage"
  ],
  "background_color": "#000",
  "theme_color": "#536878",
  "display": "standalone"
}